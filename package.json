    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "medusa-test-utils": "^1.1.38",
    "mongoose": "^6.6.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@medusajs/medusa": "latest",
    "medusa-interfaces": "latest"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "medusa-core-utils": "^1.1.33",
    "paytm-pg-node-sdk": "^1.0.4",
    "paytmchecksum": "^1.5.0",
    "typeorm": "^0.2.41"
